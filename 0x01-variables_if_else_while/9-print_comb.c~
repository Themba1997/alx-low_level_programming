#include <stdio.h>



/**

 * main - Prints a serie of numbers with commas

 *

 * Return: Always (Success);

 */

int main(void)
  
{
  
  int count;
  

  
  for (count = '0'; count <= '9'; cout++)
    
    {
      
      putchar(count);
      

      
      if (count != '9')
	
	{
	  
	  putchar(',');
	  
	  putchar(' ');
	  
	}
      
    }
  

  
  putchar('\n');
  

  
  return (0);
  
}
